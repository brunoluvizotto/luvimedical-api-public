{
  "name": "luvimedical-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node ./src/prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "dev": "NODE_ENV=dev nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --maxWorkers=2",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@cdm-logger/server": "^6.0.0",
    "@nestjs/apollo": "^10.0.16",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.16",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "4.7.0",
    "amqp-connection-manager": "^3.2.2",
    "apollo-server-express": "^3.9.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "graphql": "^16.5.0",
    "graphql-rabbitmq-subscriptions": "^1.1.0",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.4.0",
    "node-mailjet": "^5.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/cli-color": "^2.0.0",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/luxon": "^2.3.2",
    "@types/node": "^14.14.37",
    "@types/node-mailjet": "^3.3.9",
    "@types/passport-jwt": "^3.0.5",
    "@types/ramda": "^0.27.40",
    "@typescript-eslint/parser": "^4.21.0",
    "change-case": "^4.1.2",
    "cli-color": "^2.0.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prisma": "^4.7.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "~addresses/(.*)": "<rootDir>/src/addresses/$1",
      "~appointments/(.*)": "<rootDir>/src/appointments/$1",
      "~auth/(.*)": "<rootDir>/src/auth/$1",
      "~common/(.*)": "<rootDir>/src/common/$1",
      "~mailer/(.*)": "<rootDir>/src/mailer/$1",
      "~patient-records/(.*)": "<rootDir>/src/patient-records/$1",
      "~patients/(.*)": "<rootDir>/src/patients/$1",
      "~physicians/(.*)": "<rootDir>/src/physicians/$1"
    }
  }
}
